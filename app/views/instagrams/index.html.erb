
<h1>List of all your instagram posts in the database</h1>

<%= form_tag '/', method: :get do %>
  <%= search_field_tag :q, params[:q] %>
  <%= submit_tag "Search" %>
<% end %>


<%#= render 'guesses/guess_box', guesses: indiv_post.guesses %>
<%#= form_for indiv_post.guesses.build %>

<br/>

    	    <% @posts.each do |indiv_post| %>
    	<div class ="row">
    		<div class = "col-md-5">
    				<%= link_to instagram_path(indiv_post) do %> 
              <%= image_tag "#{indiv_post.photo_standard_res}" %>
            <% end %>
    		</div>
    		<div class = "col-md-7">
  				<div class = "row">
    				<p> <%= "Number of likes #{indiv_post.likes}"%> </p>
    				<p> <%= indiv_post.tastemaker.tastemaker_instagram_id%> </p>
    				<p> <%= indiv_post.tastemaker.tastemaker_full_name%> </p>
    				<p> <%= indiv_post.tastemaker.tastemaker_instagram_username%> </p>
    				<p> <%= indiv_post.tastemaker.tastemaker_profile_pict%> </p>
  				</div>
  				<div class = "row"> 
            <%= render 'post'%> -->
  				</div>
    		</div>
    	</div>
    </br></br></br>
<%end%>
