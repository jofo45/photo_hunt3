<h1>Tastemakers#index</h1>
<p>Find me in app/views/tastemakers/index.html.erb</p>



	<table class="table table-striped table-bordered table-condensed table-hover">
	    <thead>
	        <tr>
			  <th>Photo</th>
	          <th>Name / ID</th>
	          <th># of posts</th>
	          <th>influence score</th>
	          <th>fashion tastemaker</th>
	          <th>elite tastemaker</th>
	        </tr>
	    </thead>
	    <tbody>
	    <% @tastemakers.each do |indiv_tm| %>
	    	<%= simple_form_for indiv_tm do |f| %>
	        <tr>
			  <td><%= link_to tastemaker_path(indiv_tm) do %> 
              <%= image_tag "#{indiv_tm.tastemaker_profile_pict}" %>
              <% end %> </td>
	          <td><%= indiv_tm.tastemaker_full_name %> </br> 
	          	<%= indiv_tm.tastemaker_instagram_username %> </td>
	          <td><%= indiv_tm.tastemaker_counts_posts %></td>
	          <td><%= f.input :tastemaker_influence_score, label: false %></td>
	          <td><%= f.input :fashion_tastemaker, label: false %></td>
	          <td><%= f.input :elite_tastemaker, label: false, style:"padding-left: 40px;" %></td>
	          <td><%= link_to 'Show', tastemaker_path(indiv_tm) %></td>
	          <td><%= link_to 'Edit', edit_tastemaker_path(indiv_tm) %></td>
	          <td><%= link_to 'Destroy', tastemaker_path(indiv_tm), method: :delete, data: { confirm: 'Are you sure?' } %></td>
	        </tr>
	        <%end%>
	        <%end%>
	    </tbody>
	</table>
